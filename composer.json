{
    "name": "grayloon/nova-filemanager",
    "description": "Filemanager tool for Laravel Nova. Forked from InfinetyEs/nova-filemanager",
    "keywords": [
        "laravel",
        "nova",
        "filemanager"
    ],
    "homepage": "https://github.com/grayloon/nova-filemanager",
    "license": "MIT",
    "authors": [
        {
            "name": "Andy Hinkle",
            "email": "ahinkle@grayloon.com",
            "role": "Developer"
        },
        {
            "name": "Dalton McCleery",
            "email": "dmccleery@grayloon.com",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/nova": "^4.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.1",
        "laravel/framework": "^8.0|^9.0",
        "orchestra/testbench": "^6.0|^7.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Grayloon\\Filemanager\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Grayloon\\Filemanager\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Grayloon\\Filemanager\\FilemanagerServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
